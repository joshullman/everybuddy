<% if logged_in? %>
  <h4>Welcome back <%= current_user.name %></h4>
  <p><a href="/users/<%= current_user.id %>">Profile</a></p>
  <p><a href="/logout">Logout</a></p>
  <p><a href="/">Search</a></p>
<% else %>
  <p><a href="/login">Login</a></p>
  <p><a href="/users/new">Register</a></p>
<% end %>

<h1>RESULTS:</h1>
<% if !logged_in? %>
<% @users.each do |user| %>
		<p><a href="/users/<%= user.id %>"><%= user.username %></a></p>
	<% end %>
<% else %>
	<% @users.each do |user| %>
		<% if user.id != current_user.id %>
			<p><a href="/users/<%= user.id %>"><%= user.username %></a></p>
		<% end %>
	<% end %>
<% end %>